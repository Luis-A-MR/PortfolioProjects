-- Originally Created January 2022
-- Manually Added personal transaction from Capital One to pratice PostgreSQL and Tableau Visualizations

CREATE TABLE Capital_Transations (
transation_id serial PRIMARY KEY NOT NULL smallint,
item NULL varchar(255),
amount NULL decimal(7,2),
merchant NULL varchar(255),
address NULL varchar(255),
purchased_date DATE NOT NULL
)
