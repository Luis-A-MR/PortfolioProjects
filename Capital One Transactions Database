-- Fixed mistake of inputting 'NULL' instead of NULL

UPDATE capital_transations 
Set address=NULL
WHERE address='NULL'
;

-- Checking Count of Different Locations
SELECT COUNT(DISTINCT address) from capital_transations
;


SELECT address, COUNT(address) from capital_transations
WHERE address ='Miami'
OR address = 'West Palm Beach'
GROUP BY address
;

-- Fixing Spelling Mistakes

UPDATE capital_transations 
Set address='Rowland Height,California'
WHERE address='Rowland Heights, California'
;

UPDATE capital_transations 
Set address='Washington'
WHERE address='WWashington'
;

UPDATE capital_transations 
Set address='Miami, Florida'
WHERE address='Miami'
;
UPDATE capital_transations 
Set address='West Palm Beach, Florida'
WHERE address='West Palm Beach'
;

UPDATE capital_transations 
Set address='Davie, Florida'
WHERE address='Davie'
;

UPDATE capital_transations 
Set address='Doral, Florida'
WHERE address='Doral'
;

UPDATE capital_transations 
Set address='West Covina, California'
WHERE address='West Covina'
;

-- Queries for Tableau Visualizations

SELECT max(amount), address from capital_transations
GROUP BY address
;

SELECT max(amount), address from capital_transations
WHERE address IS NOT NULL
GROUP BY amount, address
ORDER BY amount desc
;


SELECT item, max(amount), address from capital_transations
WHERE address IS NOT NULL
GROUP BY item, amount, address
ORDER BY amount desc
LIMIT 10
;

SELECT COUNT(address) as Count, address from capital_transations
WHERE address LIKE '%Florida%'
GROUP BY address
;

SELECT COUNT(address) as Count, address from capital_transations
WHERE address LIKE any (array ['%Florida%','%California%'])
GROUP BY address
ORDER BY count desc
;

SELECT SUM(amount) as total, address from capital_transations
WHERE address LIKE any (array ['%Florida%','%California%'])
GROUP BY address
ORDER BY total desc
;


SELECT item, SUM(amount), address from capital_transations
WHERE item= 'boba'
GROUP BY item, address

SELECT SUM(amount) as total from capital_transations
WHERE purchased_date BETWEEN '2020-01-01' AND '2020-12-31'


SELECT SUM(amount) as total from capital_transations
WHERE purchased_date BETWEEN '2021-01-01' AND '2021-12-31'

